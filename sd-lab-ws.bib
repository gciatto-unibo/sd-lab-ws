	Author = {Idelberger, Florian and Governatori, Guido and Riveret, R{\'e}gis and Sartor, Giovanni},
	Booktitle = {Rule Technologies. Research, Tools, and Applications},
	Date-Added = {2019-03-25 11:47:55 +0100},
